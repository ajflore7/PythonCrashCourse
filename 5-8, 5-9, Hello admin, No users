usernames = ['flores', 'tony', 'admin', 'jhineric', 'anthony']
if usernames: # to check and see if the list is empty
    for username in usernames:
        if username == 'admin':
            print('Hello admin, would you like to see a status report?')
        else:
            print(f'Hello {username}, thank you for logging in again.')
else: # if list is empty the else statement with print the statement below
    print('We need to find more users!')
