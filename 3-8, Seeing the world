favorite_locations = ['Japan', 'Rome', 'Australia', 'Italy', 'Spain']

print(favorite_locations)
print(sorted(favorite_locations))
print(favorite_locations)
favorite_locations.reverse()
print(favorite_locations)
favorite_locations.reverse()
print(favorite_locations)
favorite_locations.sort()
print(favorite_locations)
favorite_locations.sort(reverse=True)
print(favorite_locations)

list = ['Rivers','Mountains', 'Plateaus', 'Cities', 'Countries', 'Language']

print(list)

list.pop(1)
print(list)

print(sorted(list))

list.append('Mountains')
print(list)

list.sort()
print(list)

list.sort(reverse=True)
print(list)

list.reverse()
print(list)

english = 'Language'
list.remove(english)
print(list)

list.remove('Rivers')
print(list)

del list[2]
print(list)

list.insert(2, 'Actor')
print(list)

list.sort()
print(list)

list[1] = 'Planets'
print(list)

print(list[-1])
print(list[-2])
print(list[-5])
