def build_profile(first, last, **user_info):
    user_info['first_name'] = first
    user_info['last_name'] = last
    return user_info
user_profile = build_profile('Tony', 'Flores',
                             major = 'CIS',
                             hobby = 'Reading',
                             fav_food = 'Pizza')
print(user_profile)
